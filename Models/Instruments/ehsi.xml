<?xml version="1.0"?>
<PropertyList>

    <path>ehsi.ac</path>




    <text>
		<name>GSPDlabel</name>
		<offsets>
			<x-m>0.0045</x-m>
			<y-m>0.050</y-m>
			<z-m>-0.042</z-m>
			<roll-deg>0</roll-deg>
			<pitch-deg>0</pitch-deg>
			<heading-deg>0</heading-deg>
		</offsets>
		<alignment>center-center</alignment>
		<axis-alignment>yz-plane</axis-alignment>
		<type>literal</type>
		<text>GSPD</text>
		<font>helvetica_bold.txf</font>
		<character-size>0.007</character-size>
		<character-aspect-ratio type="double">1.5</character-aspect-ratio>
		<font-resolution>
			<width type="int">32</width>
			<height type="int">32</height>
		</font-resolution>
	</text>

    <text>
		<name>GSPDprop</name>
		<offsets>
			<x-m>0.0045</x-m>
			<y-m>0.050</y-m>
			<z-m>-0.050</z-m>
			<roll-deg>0</roll-deg>
			<pitch-deg>0</pitch-deg>
			<heading-deg>0</heading-deg>
		</offsets>
		<alignment>center-center</alignment>
		<axis-alignment>yz-plane</axis-alignment>
		<type>number-value</type>
		<property>velocities/groundspeed-kt</property>
		<format>%03.0f</format>
		<font>helvetica_bold.txf</font>
		<character-size>0.009</character-size>
		<character-aspect-ratio type="double">1.2</character-aspect-ratio>
		<font-resolution>
			<width type="int">32</width>
			<height type="int">32</height>
		</font-resolution>
	</text>

    <text>
		<name>CRSlabel</name>
		<offsets>
			<x-m>0.0045</x-m>
			<y-m>-0.050</y-m>
			<z-m>-0.042</z-m>
			<roll-deg>0</roll-deg>
			<pitch-deg>0</pitch-deg>
			<heading-deg>0</heading-deg>
		</offsets>
		<alignment>center-center</alignment>
		<axis-alignment>yz-plane</axis-alignment>
		<type>literal</type>
		<text>CRS</text>
		<font>helvetica_bold.txf</font>
		<character-size>0.007</character-size>
		<character-aspect-ratio type="double">1.5</character-aspect-ratio>
		<font-resolution>
			<width type="int">32</width>
			<height type="int">32</height>
		</font-resolution>
	</text>

    <text>
		<name>CRSprop</name>
		<offsets>
			<x-m>0.0045</x-m>
			<y-m>-0.050</y-m>
			<z-m>-0.050</z-m>
			<roll-deg>0</roll-deg>
			<pitch-deg>0</pitch-deg>
			<heading-deg>0</heading-deg>
		</offsets>
		<alignment>center-center</alignment>
		<axis-alignment>yz-plane</axis-alignment>
		<type>number-value</type>
		<property>/instrumentation/nav/radials/selected-deg</property>
		<format>%03.0f</format>
		<font>helvetica_bold.txf</font>
		<character-size>0.009</character-size>
		<character-aspect-ratio type="double">1.2</character-aspect-ratio>
		<font-resolution>
			<width type="int">32</width>
			<height type="int">32</height>
		</font-resolution>
	</text>

    <text>
		<name>MagHDG</name>
		<offsets>
			<x-m>0.0045</x-m>
			<y-m>0.055</y-m>
			<z-m>0.036</z-m>
			<roll-deg>0</roll-deg>
			<pitch-deg>0</pitch-deg>
			<heading-deg>0</heading-deg>
		</offsets>
		<alignment>center-center</alignment>
		<axis-alignment>yz-plane</axis-alignment>
		<type>number-value</type>
		<property>/autopilot/settings/heading-bug-deg</property>
		<format>%03.0f</format>
		<font>helvetica_bold.txf</font>
		<character-size>0.007</character-size>
		<character-aspect-ratio type="double">1.2</character-aspect-ratio>
		<font-resolution>
			<width type="int">32</width>
			<height type="int">32</height>
		</font-resolution>
	</text>

    <animation>
        <name>green-text</name>
        <type>material</type>
		<object-name>GSPDlabel</object-name>
		<object-name>GSPDprop</object-name>
		<object-name>CRSlabel</object-name>
		<object-name>CRSprop</object-name>
        <object-name>MagHDG</object-name>
         	<emission>
			<red>0.1</red>
			<green>0.9</green>
			<blue>0.2</blue>
		</emission>
		<ambient>
			<red>0</red>
			<green>0</green>
			<blue>0</blue>
		</ambient>
		<diffuse>
			<red>0</red>
			<green>0</green>
			<blue>0</blue>
		</diffuse>
		<specular>
			<red>0</red>
			<green>0</green>
			<blue>0</blue>
		</specular>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>EHSI.hdg-bug</object-name>
        <property>autopilot/internal/heading-bug-error-deg</property>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>0.0</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>EHSI.compass</object-name>
        <property>orientation/heading-magnetic-deg</property>
        <axis>
            <x>1.0</x>
            <y>0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>0.0</z-m>
        </center>
    </animation>

<!--  COURSE -->

    <animation>
        <type>rotate</type>
        <object-name>EHSI.crsneedle</object-name>
        <object-name>EHSI.crsneedle1</object-name>
        <property>orientation/heading-magnetic-deg</property>
        <axis>
            <x>1.0</x>
            <y>0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>0.0</z-m>
        </center>
    </animation>


    <animation>
        <type>rotate</type>
        <object-name>EHSI.crsneedle</object-name>
        <object-name>EHSI.crsneedle1</object-name>
        <property>/instrumentation/nav/radials/selected-deg</property>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>0.0</z-m>
        </center>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>EHSI.crsneedle1</object-name>
        <property>/instrumentation/nav/heading-needle-deflection</property>
        <interpolation>
            <entry><ind>-999.0</ind><dep>-0.02</dep></entry>
            <entry><ind>-10.0</ind><dep>-0.02</dep></entry>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>10.0</ind><dep>0.02</dep></entry>
            <entry><ind>999.0</ind><dep>0.02</dep></entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>1.0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>0.0</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>EHSI.vsi</object-name>
        <property>autopilot/internal/vert-speed-fpm</property>
        <interpolation>
            <entry><ind>-3000.0</ind><dep>-40.7.0</dep></entry>
            <entry><ind>-2000.0</ind><dep>-30.5</dep></entry>
            <entry><ind>-1000.0</ind><dep>-20.3</dep></entry>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>1000.0</ind><dep>20.3</dep></entry>
            <entry><ind>2000.0</ind><dep>30.5</dep></entry>
            <entry><ind>3000.0</ind><dep>40.7</dep></entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.092</y-m>
            <z-m>0.000</z-m>
        </center>
    </animation>


</PropertyList>