<?xml version="1.0"?>
<PropertyList>

    <path>asi.ac</path>

    <animation>
        <type>rotate</type>
        <object-name>ASI.needle</object-name>
        <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>20.0</ind><dep>12.5</dep></entry>
            <entry><ind>40.0</ind><dep>76.0</dep></entry>
            <entry><ind>80.0</ind><dep>169.5</dep></entry>
            <entry><ind>120.0</ind><dep>230.0</dep></entry>
            <entry><ind>160.0</ind><dep>294.0</dep></entry>
            <entry><ind>190.0</ind><dep>342.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>-0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>
</PropertyList>