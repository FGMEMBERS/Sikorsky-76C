<?xml version="1.0"?>
<PropertyList>

    <path>asi.ac</path>

    <animation>
        <type>rotate</type>
        <object-name>ASI.needle</object-name>
        <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
        <interpolation>
          <entry><ind>0.0</ind><dep>0.0</dep></entry>
          <entry><ind>20.0</ind><dep>15.0</dep></entry>
          <entry><ind>40.0</ind><dep>75.0</dep></entry>
          <entry><ind>70.0</ind><dep>165.0</dep></entry>
          <entry><ind>80.0</ind><dep>180.0</dep></entry>
          <entry><ind>100.0</ind><dep>211.5</dep></entry>
          <entry><ind>120.0</ind><dep>243.0</dep></entry>
          <entry><ind>160.0</ind><dep>305.0</dep></entry>
          <entry><ind>200.0</ind><dep>345.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
	
	<animation>
	  <type>rotate</type>
	  <object-name>IAS.needle</object-name>
	  <property>/autopilot/settings/target-speed-kt</property>
	  <interpolation>
          <entry><ind>0.0</ind><dep>0.0</dep></entry>
          <entry><ind>20.0</ind><dep>15.0</dep></entry>
          <entry><ind>40.0</ind><dep>75.0</dep></entry>
          <entry><ind>70.0</ind><dep>165.0</dep></entry>
          <entry><ind>80.0</ind><dep>180.0</dep></entry>
          <entry><ind>100.0</ind><dep>211.5</dep></entry>
          <entry><ind>120.0</ind><dep>243.0</dep></entry>
          <entry><ind>160.0</ind><dep>305.0</dep></entry>
          <entry><ind>200.0</ind><dep>345.0</dep></entry>
	  </interpolation>
	  <axis>
	    <x>-1</x>
	    <y>0</y>
	    <z>0</z>
	  </axis>
	</animation>
	
	<!--  speed.set control -->
	  <animation>
	    <type>pick</type>
	    <object-name>ASI.face</object-name>
	    <visible>true</visible>
	    <action>
	        <button>3</button>
	        <repeatable>true</repeatable>
	        <binding>		          
		          <condition>
	            <not><property>devices/status/keyboard/shift</property></not>
	            <not><property>devices/status/keyboard/super</property></not>
			        </condition>
		          <command>property-adjust</command>
		          <property>/autopilot/settings/target-speed-kt</property>
		          <step>1</step>
		          <min>0</min>
		          <max>200</max>
		          <wrap>0</wrap>
	        </binding>
	        <binding>		          
		          <condition>
	           <or>
	            <property>devices/status/keyboard/shift</property>
	            <property>devices/status/keyboard/super</property>
	           </or>
			        </condition>
		          <command>property-adjust</command>
		          <property>/autopilot/settings/target-speed-kt</property>
		          <step>5</step>
		          <min>0</min>
		          <max>200</max>
		          <wrap>0</wrap>
	        </binding>
	        <binding>
	          <command>property-toggle</command>
	          <property>/sim/sound/switch3</property>
	        </binding>
	    </action>
	    <action>
	        <button>4</button>
	        <repeatable>true</repeatable>
	        <binding>		          
		          <condition>
	            <not><property>devices/status/keyboard/shift</property></not>
	            <not><property>devices/status/keyboard/super</property></not>
			        </condition>
	        <command>property-adjust</command>
	        <property>/autopilot/settings/target-speed-kt</property>
	        <step>-1</step>
	        <min>0</min>
	        <max>200</max>
	        <wrap>0</wrap>
	        </binding>            
	        <binding>		          
		          <condition>
	           <or>
	            <property>devices/status/keyboard/shift</property>
	            <property>devices/status/keyboard/super</property>
	           </or>
			        </condition>
	        <command>property-adjust</command>
	        <property>/autopilot/settings/target-speed-kt</property>
	        <step>-5</step>
	        <min>0</min>
	        <max>200</max>
	        <wrap>0</wrap>
	        </binding>
	    </action>
	    <action>
	      <name>increase</name>
	      <button>0</button>
	      <repeatable>true</repeatable>
	      <binding>
	        <command>property-adjust</command>
	        <property>/autopilot/settings/target-speed-kt</property>
	        <step>10</step>
	        <min>0</min>
	        <max>200</max>
	        <wrap>0</wrap>
	      </binding>
	    </action>
	    <action>
	      <name>decrease</name>
	      <button>1</button>
	      <repeatable>true</repeatable>
	      <binding>
	        <command>property-adjust</command>
	        <property>/autopilot/settings/target-speed-kt</property>
	        <step>-10</step>
	        <min>0</min>
	        <max>200</max>
	        <wrap>0</wrap>
	      </binding>
	    </action>
	  </animation>
</PropertyList>