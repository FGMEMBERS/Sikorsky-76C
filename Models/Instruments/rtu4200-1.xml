<?xml version="1.0"?>
<PropertyList>

    <path>rtu4200.ac</path>
<!-- 3d text -->

    <animation>
        <type>select</type>
        <object-name>RTU4200.screenoff</object-name>
        <condition>
            <equals>
            <property>systems/electrical/outputs/rtu[0]</property>
            <value>0</value>
            </equals>
        </condition>
    </animation>

    <text>
        <name>ComLabel</name>
        <offsets>
            <x-m>0.003</x-m>
            <y-m>-0.006</y-m>
            <z-m>0.032</z-m>
            </offsets>
        <alignment>center-center</alignment>
        <axis-alignment>yz-plane</axis-alignment>
        <type>literal</type>
        <text>COM 1</text>
        <font>helvetica_bold.txf</font>
        <character-size>0.0055</character-size>
        <character-aspect-ratio type="double">1.5</character-aspect-ratio>
        <font-resolution>
            <width type="int">32</width>
            <height type="int">32</height>
        </font-resolution>
    </text>

    <text>
        <name>ComProp1</name>
        <offsets>
            <x-m>0.003</x-m>
            <y-m>-0.034</y-m>
            <z-m>0.023</z-m>
        </offsets>
        <alignment>left-center</alignment>
        <axis-alignment>yz-plane</axis-alignment>
        <type>number-value</type>
        <property>instrumentation/comm/frequencies/selected-mhz</property>
        <format>%03.2f</format>
        <font>helvetica_bold.txf</font>
        <character-size>0.007</character-size>
        <character-aspect-ratio type="double">1.2</character-aspect-ratio>
        <font-resolution>
            <width type="int">32</width>
            <height type="int">32</height>
        </font-resolution>
    </text>

    <text>
        <name>ComProp2</name>
        <offsets>
            <x-m>0.003</x-m>
            <y-m>0.001</y-m>
            <z-m>0.023</z-m>
        </offsets>
        <alignment>left-center</alignment>
        <axis-alignment>yz-plane</axis-alignment>
        <type>number-value</type>
        <property>instrumentation/comm/frequencies/standby-mhz</property>
        <format>%03.2f</format>
        <font>helvetica_bold.txf</font>
        <character-size>0.007</character-size>
