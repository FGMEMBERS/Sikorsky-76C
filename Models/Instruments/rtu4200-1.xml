        </font-resolution>
    </text>

    <text>
        <name>ATCProp</name>
        <offsets>
            <x-m>0.003</x-m>
            <y-m>-0.034</y-m>
            <z-m>-0.012</z-m>
        </offsets>
        <alignment>left-center</alignment>
        <axis-alignment>yz-plane</axis-alignment>
        <type>number-value</type>
        <property>instrumentation/transponder/id-code</property>
