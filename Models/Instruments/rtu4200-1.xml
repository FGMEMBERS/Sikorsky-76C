    <text>
        <name>ATCLabel</name>
        <offsets>
            <x-m>0.003</x-m>
            <y-m>-0.024</y-m>
            <z-m>-0.003</z-m>
            </offsets>
        <alignment>center-center</alignment>
        <axis-alignment>yz-plane</axis-alignment>
        <type>literal</type>
        <text>ATC 1</text>
        <font>helvetica_bold.txf</font>
