        <font-resolution>
            <width type="int">32</width>
            <height type="int">32</height>
        </font-resolution>
    </text>

    <text>
        <name>NavProp1</name>
        <offsets>
            <x-m>0.003</x-m>
            <y-m>-0.034</y-m>
