            <splash-texture>Aircraft/Sikorsky-76C/splash3.rgb</splash-texture>
        </startup>

        <model>
            <path>Aircraft/Sikorsky-76C/Models/s76c.xml</path>
            <variant type="int">2</variant>
            <texture type = "string">S76livery.rgb</texture>
        </model>
    </sim>
</PropertyList>